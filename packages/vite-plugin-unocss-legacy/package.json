{
  "name": "vite-plugin-unocss-legacy",
  "version": "0.58.3",
  "description": "The Vite plugin for UnoCSS",
  "author": "xxllxhdj",
  "license": "MIT",
  "keywords": [
    "unocss",
    "vite",
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0-0 || ^4.0.0 || ^5.0.0-0"
  },
  "dependencies": {
    "@ampproject/remapping": "^2.2.1",
    "@rollup/pluginutils": "^5.1.0",
    "@unocss/config": "workspace:*",
    "@unocss/core": "workspace:*",
    "@unocss/inspector": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/scope": "workspace:*",
    "@unocss/transformer-directives": "workspace:*",
    "chokidar": "^3.5.3",
    "fast-glob": "^3.3.2",
    "magic-string": "^0.30.5"
  },
  "devDependencies": {
    "@unocss/shared-integration": "workspace:*",
    "vite": "^5.0.10"
  }
}
